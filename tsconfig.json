{
  "compilerOptions": {
    "allowSyntheticDefaultImports": false,
    "declaration": true,
    "esModuleInterop": false,
    "module": "es2015",
    "newLine": "lf",
    "noEmitOnError": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "sourceMap": true,
    "strict": true,
    "strictNullChecks": true,
    "target": "es2015"
  }
}
